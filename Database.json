    "views": [
        {
            "name": "EntityParameterViews",
            "query": [
                "select",
                "  ep.Id,",
                "  ep.EntityTypeGuid,",
                "  ep.EntityGuid,",
                "  po.PageTitle,",
                "  po.MetaTitle,",
                "  po.MetaDescription,",
                "  po.Keywords,",
