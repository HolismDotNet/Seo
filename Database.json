{
    "name": "Seo",
    "tables": [
        {
            "name": "ParametersObjects",
            "columns": [
                {
                    "name": "PageTitle"
                },
                {
                    "name": "MetaTitle"
                },
                {
                    "name": "MetaDescription",
                    "type": "longText"
                },
                {
                    "name": "Keywords",
                    "nullable": true
                },
                {
                    "name": "PageDescription",
                    "type": "longText"
                },
                {
                    "name": "ExtraData",
                    "type": "longText",
                    "nullable": true
                }
            ]
        },
        {
            "name": "EntityTypeParameters",
            "columns": [
                {
                    "name": "EntityTypeGuid",
                    "isUnique": true
                },
                {
                    "name": "ParametersObjectId"
                }
            ]
        },
        {
            "name": "EntityParameters",
            "columns": [
                {
                    "name": "EntityTypeGuid"
                },
                {
                    "name": "EntityGuid"
                },
                {
                    "name": "ParametersObjectId"
                }
            ],
            "indexes": [
                {
                    "columns": [
                        "EntityTypeGuid",
                        "EntityGuid"
                    ],
                    "isUnique": true
                }
            ]
        },
        {
            "name": "QueryParameters",
            "columns": [
                {
                    "name": "SortedParameters",
                    "isUnique": true
                },
                {
                    "name": "ParametersObjectId"
                }
            ]
        },
        {
            "name": "PathParameters",
            "columns": [
                {
                    "name": "Path",
                    "isUnique": true
                },
                {
                    "name": "ParametersObjectId"
                }
            ]
        }
    ],
    "views": [
        {
            "name": "EntityParameterViews",
            "query": [
                "select",
                "  ep.Id,",
                "  ep.EntityTypeGuid,",
                "  ep.EntityGuid,",
                "  po.PageTitle,",
                "  po.MetaTitle,",
                "  po.MetaDescription,",
                "  po.Keywords,",
                "  po.PageDescription",
                "from EntityParameters ep",
                "inner join ParametersObjects po",
                "on ep.ParametersObjectId = po.Id"
            ]
        },
        {
            "name": "PathParameterViews",
            "query": [
                "select",
                "  pp.Id,",
                "  pp.Path,",
                "  po.PageTitle,",
                "  po.MetaTitle,",
                "  po.MetaDescription,",
                "  po.Keywords,",
                "  po.PageDescription",
                "from PathParameters pp",
                "inner join ParametersObjects po",
                "on pp.ParametersObjectId = po.Id"
            ]
        },
        {
            "name": "QueryParameterViews",
            "query": [
                "select",
                "  qp.Id,",
                "  qp.SortedParameters,",
                "  po.PageTitle,",
                "  po.MetaTitle,",
                "  po.MetaDescription,",
                "  po.Keywords,",
                "  po.PageDescription",
                "from QueryParameters qp",
                "inner join ParametersObjects po",
                "on qp.ParametersObjectId = po.Id"
            ]
        }
    ]
}